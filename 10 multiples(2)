import java.util.*;


class test
{
public static void main(String[] args)
{

       Scanner sc=new Scanner(System.in);
       System.out.println("enter number");
       int n=sc.nextInt();
       
       for(int i=1;i<=10;i++)
       {
              int num=n*i;
              
              System.out.println(n+"*"+i+"="+num);
              
              
              
       }
       

}

} 

